<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.bjtu.jzlj.mapper.ConstructionWasteInfoMapper">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.edu.bjtu.jzlj.domain.ConstructionWasteInfo">
        <id column="CONSTRUCTION_WASTE_ID" property="constructionWasteId"/>
        <result column="SOURCE_ID" property="sourceId"/>
        <result column="CONSTRUCTION_WASTE_WEIGHT" property="constructionWasteWeight"/>
        <result column="CONSTRUCTION_WASTE_UNIT" property="constructionWasteUnit"/>
        <result column="CONTENT" property="content"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        CONSTRUCTION_WASTE_ID, SOURCE_ID, CONSTRUCTION_WASTE_WEIGHT, CONSTRUCTION_WASTE_UNIT, CONTENT
    </sql>

</mapper>
